<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tyut.domain">
	<class name="User" table="t_user">
		
		<id name="userId" column="user_id" type="integer">
			<generator class="native"/>
		</id>
		
		<property name="username" column="username" type="string"/>
		
		<property name="password" column="password" type="string"/>
		
		<property name="gender" column="gender" type="string"/>
		
		<property name="schoolId" column="school_id" type="string"/>
		
		<property name="birthday" column="birthday" type="string"/>
		
		<property name="createTime" column="create_time" type="string"/>
		
	</class>
</hibernate-mapping>